{
  "manifest_version": 3,
  "name": "Foureyes",
  "description": "Learning what you are learning while browsing",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "workers/service-worker.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "alarms",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content/content.js"
      ]
    }
  ]
}