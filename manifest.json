{
  "manifest_version": 3,
  "name": "Foureyes",
  "description": "Learning what you are learning while browsing",
  "version": "1.0",
  "key": "",
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "oauth2": {
    "client_id": "258123725523-h9bfbasesfqg203rorq4prvnb29qlr13.apps.googleusercontent.com",
    "scopes":["email","openid","profile"]
  },
  "background": {
    "service_worker": "workers/service-worker.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "alarms",
    "webNavigation",
    "tabs",
    "offscreen",
    "identity"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content/html2canvas.min.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content/content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}