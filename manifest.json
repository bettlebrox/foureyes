{
  "manifest_version": 3,
  "name": "Foureyes",
  "description": "Learning what you are learning while browsing",
  "version": "1.0",
  "key": "",
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "oauth2": {
    "client_id": "258123725523-e9g2j70es4083n0if9cg59cbdn9h898q.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/userinfo.email"]
  },
  "background": {
    "service_worker": "workers/service-worker.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "alarms",
    "webNavigation",
    "tabs",
    "offscreen",
    "identity"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content/content.js"
      ]
    }
  ]
}